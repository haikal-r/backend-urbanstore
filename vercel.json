{
  "builds": [
    {
      "src": "./index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/",
      "methods": ["GET", "POST", "PUT", "PATCH", "DELETE", "OPTIONS"]
    }
  ],
  "build": {
    "env": {
      "VERCEL_BUILDER_NO_CACHE": "1"
    },
    "command": "npm run build"
  }
}
